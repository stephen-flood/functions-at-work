              \draw[color=red,fill] (-1,{f(-1)}) circle (3pt);
              \draw[color=red,fill] (0,{f(0)}) circle (3pt);
              \draw[color=red,fill] (1,{f(1)}) circle (3pt);
              \draw[color=red,fill] (2,{f(2)}) circle (3pt);

            \end{tikzpicture}
          </latex-image>				
        </image>			
      </solution>
    </task>
    <task>
      <statement>
        <p>
          Let <m>f(x) = 5\cdot 1.1^x</m>.
          Compute  <m>f(-1)</m>, <m>f(0)</m>, <m>f(1)</m>, and <m>f(2)</m>.
