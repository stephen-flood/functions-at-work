            \draw [fill=black,opacity=0.2]	
              plot [smooth,domain=0:{pi}] ({\x},{f(\x)}) 
              -- cycle;
            \draw [fill=black,opacity=0.2]	
              plot [smooth,domain={pi}:{2*pi}] ({\x},{f(\x)}) 
              -- cycle;
